@model ESFA.DC.Web.Operations.Topics.Data.Entities.JobSubscriptionTask

@{
    ViewData["Title"] = "Edit";
    Layout = null;
}

<h1>Edit</h1>

<h4>JobSubscriptionTask</h4>

<h4>@Model.JobTopic.Collection.Name</h4>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="JobTopicTaskId" />
            <div class="form-group">
                <label asp-for="JobTopicId" class="control-label"></label>
                <select asp-for="JobTopicId" class="form-control" asp-items="ViewBag.JobTopicId" disabled="disabled"></select>
                <span asp-validation-for="JobTopicId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TaskName" class="control-label"></label>
                <input asp-for="TaskName" class="form-control" disabled="disabled"/>
                <span asp-validation-for="TaskName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TaskOrder" class="control-label"></label>
                <input asp-for="TaskOrder" class="form-control" disabled="disabled"/>
                <span asp-validation-for="TaskOrder" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Enabled" class="control-label"></label>
                <input asp-for="Enabled" class="form-control" />
                <span asp-validation-for="Enabled" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
