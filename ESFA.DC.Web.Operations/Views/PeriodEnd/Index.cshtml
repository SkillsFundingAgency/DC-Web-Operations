<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <h1 class="govuk-heading-xl">
            Period end
            <span class="govuk-caption-xl">RO6 closed 22 April 2019 at 18.05</span>
        </h1>
        
        
        @using (Html.BeginForm("StartPeriodEnd", "PeriodEnd", FormMethod.Post, new {@class = "app-task-list__section"}))
        {
            <input type="submit" class="govuk-button customised" id="start" value="Start Period End">
        }

        <ol id="pathContainer" class="app-task-list">
        </ol>
    </div>
</div>

@section PageScripts
{
    <script src="~/assets/js/signalr/dist/browser/signalr.js"></script>
    <script src="~/assets/js/hubs/periodEnd.js"></script>
}