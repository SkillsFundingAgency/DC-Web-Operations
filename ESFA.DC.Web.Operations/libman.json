{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "provider": "filesystem",
      "library": "node_modules/@aspnet/signalr",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "filesystem",
      "library": "node_modules/handlebars/",
      "destination": "wwwroot/lib/handlebars/",
      "files": [
        "dist/handlebars.js"
      ]
    },
    {
      "provider": "filesystem",
      "library": "node_modules/chart.js",
      "destination": "wwwroot/lib/chartjs/",
      "files": [
        "dist/Chart.bundle.min.js"
      ]
    },
    {
      "provider": "filesystem",
      "library": "node_modules/govuk-frontend/assets/fonts/",
      "destination": "wwwroot/lib/govuk-frontend/assets/fonts"
    },
    {
      "provider": "filesystem",
      "library": "node_modules/govuk-frontend/assets/images/",
      "destination": "wwwroot/lib/govuk-frontend/assets/images"
    }
  ]
}