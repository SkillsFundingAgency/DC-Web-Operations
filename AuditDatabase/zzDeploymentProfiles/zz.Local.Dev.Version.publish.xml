<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>OpsAudit</TargetDatabaseName>
    <DeployScriptFileName>ESFA.DC.Web.Operations.AuditDatabase.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=.;Integrated Security=True;Persist Security Info=False;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=False;TrustServerCertificate=False</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
  </PropertyGroup>
  <ItemGroup>
    <SqlCmdVariable Include="BUILD_BRANCHNAME">
      <Value>Local</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="BUILD_BUILDNUMBER">
      <Value>0.0.0</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="DSCIUserPassword">
      <Value>NotUseLocally##</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="RELEASE_RELEASENAME">
      <Value>From Local Dev Machine</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="ROUserPassword">
      <Value>NotUseLocally</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="RWUserPassword">
      <Value>NotUseLocally#</Value>
    </SqlCmdVariable>
  </ItemGroup>
</Project>